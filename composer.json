{
    "name": "marksamp/nfse",
    "description": "Cliente PHP para API de Nota Fiscal Eletrônica de Serviço da FocusNFe",
    "type": "library",
    "keywords": ["nfse", "nota fiscal", "focusnfe", "api", "eletrônica"],
    "license": "MIT",
    "authors": [
        {
            "name": "Márcio Sampaio",
            "email": "marcio_ce@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FocusNFe\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}