{
    "name": "focusnfe/nfse-client",
    "description": "Cliente PHP para API de Nota Fiscal Eletrônica de Serviço da FocusNFe",
    "type": "library",
    "keywords": ["nfse", "nota fiscal", "focusnfe", "api", "eletrônica"],
    "license": "MIT",
    "authors": [
        {
            "name": "Seu Nome",
            "email": "seu.email@exemplo.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "FocusNFe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FocusNFe\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}